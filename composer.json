{
    "name": "viget/craft-viget-base",
    "description": "Viget Base Module",
    "type": "yii2-module",
    "version": "1.2.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms"
    ],
    "support": {
        "docs": "https://github.com/vigetlabs/craft-viget-base/blob/main/README.md",
        "issues": "https://github.com/vigetlabs/craft-viget-base/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Trevor Davis",
            "homepage": "https://www.viget.com/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.3.0",
        "sleiman/airtable-php": "^2.4"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require-dev": {
        "vlucas/phpdotenv": "^4.1.0",
        "codeception/codeception": "^4.1.1",
        "codeception/module-asserts": "^1.1.1",
        "codeception/lib-asserts": "^1.1.0",
        "codeception/module-yii2": "^1.1.0",
        "phpunit/phpunit": "^7.5.20",
        "codeception/module-phpbrowser": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "viget\\base\\": "src/",
            "vigetbasetests\\fixtures\\": "tests/fixtures/"
        }
    }
}
